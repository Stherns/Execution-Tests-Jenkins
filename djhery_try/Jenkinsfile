pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/Stherns/Execution-Tests-Robot-Framework-depuis-GitHub.git'
            }
        }
        stage('Run Robot Tests') {
            steps {
                sh 'robot -d results djhery_try/'
            }
        }
        stage('Publish Results') {
            steps {
                robot plugin: 'true', outputPath: 'results'
            }
        }
    }
}

