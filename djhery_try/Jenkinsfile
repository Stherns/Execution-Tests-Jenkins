pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/your-username/your-repo.git'
            }
        }
        stage('Run Robot Tests') {
            steps {
                sh 'robot -d results tests/'
            }
        }
        stage('Publish Results') {
            steps {
                robot plugin: 'true', outputPath: 'results'
            }
        }
    }
}

